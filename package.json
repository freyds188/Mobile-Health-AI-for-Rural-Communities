{
  "name": "health-ai-mobile-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "optimize:assets": "expo-optimize",
    "build:web": "expo export:web",
    "train:basic": "npx tsx src/scripts/trainBasicModel.ts",
    "train:advanced": "npx tsx src/scripts/trainAdvancedModel.ts",
    "train:real": "npx tsx src/scripts/trainWithRealData-node.ts",
    "train:nlp": "npx tsx src/scripts/trainNLPModel.ts",
    "seed:nlp": "npx tsx src/scripts/seedNLPTrainingData.ts",
    "seed:nlp:all": "npx tsx src/scripts/seedNLPTrainingData.ts && npx tsx src/scripts/trainNLPModel.ts",
    "generate:conv-dataset": "npx tsx src/scripts/generateConversationalDataset.ts",
    "generate:sentiment-dataset": "npx tsx src/scripts/generateSentimentDataset.ts",
    "seed:sentiment-lexicon": "npx tsx src/scripts/seedSentimentLexicon.ts",
    "train:app": "npx tsx src/scripts/trainWithRealData.ts",
    "validate:model": "npx tsx src/scripts/validateModel.ts",
    "deploy:model": "npx tsx src/scripts/deployModel-node.ts",
    "generate:model-manifest": "npx tsx src/scripts/generateModelManifest.ts",
    "test:health-data": "npx tsx src/scripts/testHealthDataLogging.ts",
    "analyze:dataset": "echo 'Dataset analyzer moved into docs; run dataset_analyzer.py manually if needed'"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo-google-fonts/nunito": "^0.2.3",
    "@expo-google-fonts/poppins": "^0.2.3",
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "expo": "~49.0.15",
    "expo-crypto": "~12.4.1",
    "expo-file-system": "~15.4.5",
    "expo-font": "~11.4.0",
    "expo-linear-gradient": "~12.3.0",
    "expo-localization": "~14.3.0",
    "expo-notifications": "^0.20.1",
    "expo-secure-store": "~12.3.1",
    "expo-sharing": "~11.4.0",
    "expo-sqlite": "~11.3.3",
    "expo-status-bar": "~1.6.0",
    "i18n-js": "^4.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-chart-kit": "^6.12.0",
    "react-native-crypto-js": "^1.0.0",
    "react-native-date-picker": "^4.3.3",
    "react-native-dotenv": "^3.4.9",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-gifted-charts": "^1.3.26",
    "react-native-modal": "^13.0.1",
    "react-native-picker-select": "^8.1.0",
    "react-native-progress": "^5.0.1",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-svg": "13.9.0",
    "react-native-vector-icons": "^10.0.2",
    "react-native-web": "~0.19.6",
    "sql.js": "^1.8.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.72.2",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.6.2",
    "typescript": "^5.1.3"
  },
  "private": true
}
